{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/jsx-props-no-spreading": [0],
    "jsx-a11y/media-has-caption": [0],
    "comma-dangle": [0],
    "object-curly-newline": [0],
    "implicit-arrow-linebreak": [0]
  }
}
